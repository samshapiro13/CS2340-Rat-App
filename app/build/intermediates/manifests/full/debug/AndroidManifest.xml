<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shouryakhare.cs2340_rat_app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="25" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.shouryakhare.cs2340_rat_app.Controller.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.shouryakhare.cs2340_rat_app.Controller.LoginSuccessfulActivity"
            android:label="@string/title_activity_login_successful"
            android:parentActivityName="com.example.shouryakhare.cs2340_rat_app.Controller.MainActivity"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shouryakhare.cs2340_rat_app.Controller.MainActivity" />
        </activity>
        <activity
            android:name="com.example.shouryakhare.cs2340_rat_app.Controller.RegistrationActivity"
            android:label="RegistrationActivity"
            android:parentActivityName="com.example.shouryakhare.cs2340_rat_app.Controller.MainActivity"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shouryakhare.cs2340_rat_app.Controller.MainActivity" />
        </activity>
    </application>

</manifest>